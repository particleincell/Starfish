<!-- material interactions file -->
<material_interactions>

<!-- surface interaction -->
<surface_hit source="O+" target="SS">
<product>O</product>
<model>cosine</model>
<prob>0.9</prob>
<c_accom>0.5</c_accom>
<c_rest>0.9</c_rest>
</surface_hit>

<sputtering>
<source>O+</source>
<target>SS</target>
<model>const</model>
<yield>0.1</yield>
<energy>1</energy> 
</sputtering>

<!-- chemical reaction -->
<chemistry name="ionization" >
<sources>O,e-</sources>
<products>iO+,2*e-</products>
<rate type="poly" is_sigma="true">
 <coeffs>1,0</coeffs>
 <output_wrappers>1e-9</output_wrappers>
 <dep_var>T.e-</dep_var>
<!--	<input_wrappers>energy,JtoEv</input_wrappers> -->
</rate>
</chemistry> 

<!-- MCC charge exchange -->
<mcc model="cex">
<source>O+</source>
<target>O</target>
<sigma>const</sigma>
<sigma_coeffs>1e-7</sigma_coeffs>
</mcc> 


</material_interactions>
